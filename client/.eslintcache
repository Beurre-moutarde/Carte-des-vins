`authorization` header\nconst authLink = setContext((_, { headers }) => {\n  const token = localStorage.getItem(\"id_token\");\n  return {\n    headers: {\n      ...headers,\n      authorization: token ? `Bearer ${token}` : \"\",\n    },\n  };\n});\n\n// execute the `authLink` middleware prior to making the request to our GraphQL API\nconst client = new ApolloClient({\n  link: authLink.concat(httpLink),\n  cache: new InMemoryCache(),\n});\n\nfunction App() {\n  const [isModalVisible, setIsModalVisible] = useState(false);\n  const [modalTitle, setModalTitle] = useState('');\n  const [modalContent, setModalContent] = useState(null);\n\n  const handleModalCancel = () => {\n    setIsModalVisible(false);\n  };\n\n  const showModal = (title, content) => {\n    setModalTitle(title);\n    setModalContent(content);\n    setIsModalVisible(true);\n  };\n  return (\n    <ApolloProvider client={client}>\n          <Layout>\n      <Header>\n        <div className=\"logo\" />\n        <Menu theme=\"dark\" mode=\"horizontal\" defaultSelectedKeys={['1']}>\n          <Menu.Item key=\"1\">Home</Menu.Item>\n          <Menu.Item key=\"2\" onClick={() => showModal('Sign Up', <SignupForm />)}>Sign Up</Menu.Item>\n          <Menu.Item key=\"3\" onClick={() => showModal('Log In', <LoginForm />)}>Log In</Menu.Item>\n        </Menu>\n      </Header>\n      <Router>\n        {/* <AppLayout> */}\n          <Routes>\n            <Route  path=\"/\" element={<Home />} />\n            <Route path=\"/profile\" element={<Profile />} />\n            <Route render={() => <h1 className=\"display-2\">Wrong page!</h1>} />\n          </Routes>\n        {/* </AppLayout> */}\n      </Router>\n      <Footer style={{ textAlign: 'center' }}>Ant Design Â©2023 Created by Me</Footer>\n\n      <Modal\n        title={modalTitle}\n        visible={isModalVisible}\n        onCancel={handleModalCancel}\n        footer={null}\n      >\n        {modalContent}\n      </Modal>\n    </Layout>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n\n","/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/utils/queries.js",[],"/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/utils/auth.js",["44"],"import decode from 'jwt-decode';\n\nclass AuthService {\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token);\n  }\n\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();","/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/components/LoginForm.js",[],"/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/pages/Home.js",[],"/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/components/SignUpForm.js",[],"/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/utils/mutations.js",[],"/Users/christopher/Desktop/Bootcamp-assignments/FINAL-PROJECT/Carte-des-vins/client/src/pages/Profile.js",["45","46","47"],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53",